<div class="card">
  <div class="card-body">
    <%= link_to new_student_path do %>
    <button type="button" class="btn btn-outline-dark">
    Add Student
    </button>
    <% end %><br>

    <h2 class="card-title">Students</h2>

    <% @student.each do |t| %>
      <a href="/students/<%= t.id %>">
      <%= t.first_name %> 
      <%= t.last_name %>
      </a><br>
    <% end %>
    
    <%= link_to "Back", :back %>
  </div>
</div>

